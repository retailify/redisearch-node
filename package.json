    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "buffers": "^0.1.1",
    "redis": "^2.8.0"
